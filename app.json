{
  "expo": {
    "name": "blog-post-portal",
    "slug": "blog-post-portal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Avatar.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Avatar.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.irsage.blogpost"
    },
    "web": {
      "favicon": "./assets/Avatar.png"
    }
  }
}
